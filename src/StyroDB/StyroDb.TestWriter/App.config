<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="DbWriter" type="System.Configuration.NameValueSectionHandler" />
    <section name="Distributor" type="System.Configuration.NameValueSectionHandler" />
  </configSections>

  <DbWriter>
    <add key="host" value="localhost" />
    <add key="portForDistributor" value="12331" />
    <add key="portForCollector" value="12332" />
    <add key="countTreads" value="1" />
    <add key="countReplics" value="1" />
  </DbWriter>

  <Distributor>
    <add key="host" value="localhost" />
    <add key="portForProxy" value="12331" />
    <add key="portForDbWriter" value="12332" />
    <add key="countTreads" value="1" />
    <add key="countReplics" value="1" />
  </Distributor>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  
</configuration>